<?xml version='1.0' encoding='UTF-8'?>
<addons><addon id="repository.prichym" name="PrichyM's Repository" version="1.0.0" provider-name="PrichyM">
    <extension point="xbmc.addon.repository" name="PrichyM's Repository">
        <dir minversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/PrichyM/kodirepo/main/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/PrichyM/kodirepo/main/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/PrichyM/kodirepo/main/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>PrichyM's Repository</summary>
        <description>A repository of personal add-ons</description>
        <platform>all</platform>
        <language>cs</language>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
